@model Employee[]

<form asp-controller="employees" asp-action="Invade">
    <input type="submit" value="Invade" />
</form>

    <ul>
        @foreach (var employee in Model)
        {
            <li><a asp-controller="Employees" asp-action="Details" asp-route-id="@employee.Id">@employee.Name</a></li>
        }


    </ul>

